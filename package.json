{
	"name": "scribeai",
	"displayName": "ChatGPT & GPT extension - ScribeAI",
	"description": "ðŸš€ Use ChatGPT & GPT right inside VSCode to enhance and automate your coding with AI-powered assistance",
	"version": "1.2.1",
	"publisher": "AndrewZhang",
	"private": true,
	"icon": "resources/chatbot.png",
	"license": "Apache-2.0",
	"repository": {
		"type": "git",
		"url": "https://github.com/ajikan/ScribeAI"
	},
	"engines": {
		"vscode": "^1.65.0"
	},
	"sponsor": {
		"url": "https://ko-fi.com/pixelperfectlabs"
	},
	"categories": [
		"Programming Languages",
		"Snippets",
		"Other",
		"Machine Learning",
		"Notebooks"
	],
	"keywords": [
		"chatgpt",
		"gpt3",
		"gpt3.5",
		"gpt4",
		"openai",
		"copilot",
		"ai",
		"refactor",
		"bard",
		"completion",
		"javascript",
		"typescript",
		"web development"
	],
	"galleryBanner": {
		"color": "#041621",
		"theme": "dark"
	},
	"activationEvents": [
		"onStartupFinished"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "mywiki.createNote",
				"title": "Create Note",
				"enablement": "!commentIsEmpty"
			},
			{
				"command": "mywiki.replyNote",
				"title": "Create Note",
				"enablement": "!commentIsEmpty"
			},
			{
				"command": "mywiki.askAI",
				"title": "Ask AI",
				"enablement": "!commentIsEmpty"
			},
			{
				"command": "mywiki.aiEdit",
				"title": "AI Edit",
				"enablement": "!commentIsEmpty"
			},
			{
				"command": "mywiki.extendDocString",
				"title": "Extend",
				"enablement": "true"
			},
			{
				"command": "mywiki.editNote",
				"title": "Edit",
				"icon": {
					"dark": "resources/edit_inverse.svg",
					"light": "resources/edit.svg"
				}
			},
			{
				"command": "mywiki.deleteNote",
				"title": "Delete",
				"icon": {
					"dark": "resources/close_inverse.svg",
					"light": "resources/close.svg"
				}
			},
			{
				"command": "mywiki.deleteNoteComment",
				"title": "Delete",
				"icon": {
					"dark": "resources/close_inverse.svg",
					"light": "resources/close.svg"
				}
			},
			{
				"command": "mywiki.saveNote",
				"title": "Save"
			},
			{
				"command": "mywiki.cancelsaveNote",
				"title": "Cancel"
			},
			{
				"command": "mywiki.dispose",
				"title": "Remove All Notes"
			}
		],
		"menus": {
			"commandPalette": [
				{
					"command": "mywiki.createNote",
					"when": "false"
				},
				{
					"command": "mywiki.replyNote",
					"when": "false"
				},
				{
					"command": "mywiki.askAI",
					"when": "false"
				},
				{
					"command": "mywiki.aiEdit",
					"when": "false"
				},
				{
					"command": "mywiki.extendDocString",
					"when": "true"
				},
				{
					"command": "mywiki.deleteNote",
					"when": "false"
				},
				{
					"command": "mywiki.deleteNoteComment",
					"when": "false"
				}
			],
			"comments/commentThread/title": [
				{
					"command": "mywiki.deleteNote",
					"group": "navigation",
					"when": "commentController == comment-scribeai && !commentThreadIsEmpty"
				}
			],
			"comments/commentThread/context": [
				{
					"command": "mywiki.createNote",
					"group": "inline@5",
					"when": "commentController == comment-scribeai && commentThreadIsEmpty"
				},
				{
					"command": "mywiki.replyNote",
					"group": "inline@4",
					"when": "commentController == comment-scribeai && !commentThreadIsEmpty"
				},
				{
					"command": "mywiki.askAI",
					"group": "inline@3",
					"when": "commentController == comment-scribeai"
				},
				{
					"command": "mywiki.aiEdit",
					"group": "inline@2",
					"when": "commentController == comment-scribeai"
				},
				{
					"command": "mywiki.extendDocString",
					"group": "inline@1",
					"when": "commentController == comment-scribeai"
				}
			],
			"comments/comment/title": [
				{
					"command": "mywiki.editNote",
					"group": "group@1",
					"when": "commentController == comment-scribeai"
				},
				{
					"command": "mywiki.deleteNoteComment",
					"group": "group@2",
					"when": "commentController == comment-scribeai && comment == canDelete"
				}
			],
			"comments/comment/context": [
				{
					"command": "mywiki.cancelsaveNote",
					"group": "inline@1",
					"when": "commentController == comment-scribeai"
				},
				{
					"command": "mywiki.saveNote",
					"group": "inline@2",
					"when": "commentController == comment-scribeai"
				}
			]
		},
		"configuration": {
			"title": "ScribeAI",
			"properties": {
				"scribeai.ApiKey": {
					"type": "string",
					"default": "",
					"scope": "application",
					"markdownDescription": "Your personal OpenAI API key. If you don't have one, generate a new one [here](https://beta.openai.com/account/api-keys).\n\nDon't forget to [restart your extension](command:workbench.action.reloadWindow) once you finished configuring so that your extension gets loaded with the new configurations."
				},
				"scribeai.models": {
					"type": "string",
					"default": "gpt-3.5-turbo-16k",
					"enum": [
						"gpt-3.5-turbo",
						"gpt-3.5-turbo-16k",
						"gpt-4"
					],
					"enumDescriptions": [
						"GPT-3.5 Turbo is a model specifically optimized for dialogues and is capable of understanding and generating natural language.",
						"GPT-3.5-Turbo-16K is a version of the GPT-3.5 Turbo model with more parameters, potentially offering higher quality dialogues and text generation.",
						"GPT-4 is the most advanced AI model to date, with stronger reasoning and cognitive abilities."
					],
					"markdownDescription": "Choose the OpenAI model you want to use for generating your AI responses.\n\nYou can find more details about each model and their pricing [here](https://platform.openai.com/docs/models)."
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint \"src/**/*.ts\""
	},
	"devDependencies": {
		"@types/node": "^16.18.33",
		"@types/vscode": "~1.65.0",
		"@typescript-eslint/eslint-plugin": "^5.59.7",
		"@typescript-eslint/parser": "^5.59.7",
		"eslint": "^8.41.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"openai": "^3.2.1"
	}
}
